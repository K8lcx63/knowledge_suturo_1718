<?xml version="1.0" encoding="utf-8"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY knowrob "http://knowrob.org/kb/knowrob.owl#" >
]>

<rdf:RDF xmlns="http://knowrob.org/kb/iai-kitchen.owl#"
     xml:base="http://knowrob.org/kb/iai-kitchen.owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:knowrob="http://knowrob.org/kb/knowrob.owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  
    <owl:Ontology rdf:about="http://knowrob.org/kb/IAI-kitchen-knowledge.owl">
        <owl:imports rdf:resource="package://knowrob_common/owl/knowrob.owl"/>
    </owl:Ontology>
    
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:DatatypeProperty rdf:about="&knowrob;boundingBoxSize"/>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://knowrob.org/kb/knowrob.owl#PackagingContainerProduct -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob.owl#PackagingContainerProduct"/>

    <!-- http://knowrob.org/kb/suturo_object.owl#IceTeaPackage -->

    <owl:Class rdf:about="http://knowrob.org/kb/suturo_object.owl#IceTeaPackage">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/suturo_object.owl#Package"/>
    </owl:Class>
    
    <!-- http://knowrob.org/kb/suturo_object.owl#Package -->

    <owl:Class rdf:about="http://knowrob.org/kb/suturo_object.owl#Package">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob.owl#PackagingContainerProduct"/>
    </owl:Class>

    <!-- http://knowrob.org/kb/suturo_object.owl#PfannerIceTeaPackage -->

    <owl:Class rdf:about="http://knowrob.org/kb/suturo_object.owl#PfannerIceTea2LPackage">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/suturo_object.owl#IceTeaPackage"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&knowrob;boundingBoxSize"/>
                <owl:hasValue rdf:datatype="&xsd;string">0.09 0.26 0.09</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>  
    </owl:Class>
</rdf:RDF>